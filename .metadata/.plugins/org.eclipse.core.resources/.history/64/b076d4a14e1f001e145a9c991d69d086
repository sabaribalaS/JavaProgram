package looping_practice;

public class Looping_prac {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Looping_prac pr = new Looping_prac();
//		pr.print1(); 
//		pr.printno();
//		pr.printno1();
//		pr.print3();
//		pr.print4(); //multiple of 3 and 5
//		pr.divided_of_3();
		pr.multiples_or3or5();
		

	}

	private void multiples_or3or5() {
		// TODO Auto-generated method stub
		for(int no=1;no<=20;no++) {
			if(no%3==0 || no%5==0 ) {
				System.out.print(no+" ");
			}
		}
		
	}

	private void divided_of_3() {
		// TODO Auto-generated method stub
		for (int no=1;no<=20;no++) {
			if(no%3==0) {
				System.out.print(no+" ");
			}
		}
		
	}

	private void print4() {
		// TODO Auto-generated method stub
//		Multiples of 3 and 5
		for(int s=1;s<=40;s++) {
			if(s%3==0 && s%5==0) {
				System.out.print(s+" ");
			}
			
		}
		
	}

	private void print3() {
		// TODO Auto-generated method stub
//		3 6 9 12 15
		for(int no=3;no<=15;no=no+3) {
			System.out.print(no+" ");
		}
		
	}

	private void printno1() {
		// TODO Auto-generated method stub
//		1 3 5 7 9 
		int i=1;
		while(i<=9) {
			
			System.out.print(i+" ");
			i=i+2;
		}
		
		
	}

	private static void printno() {
		// TODO Auto-generated method stub
		for(int i=1;i<=5;i++) {
			System.out.print(i+" " );
		}
		
		
	}

	{
		// TODO Auto-generated method stub
		
	}

	private void print1() {
		// TODO Auto-generated method stub
//		output:1  1   1   1   1
		for(int i=1;i<=5;i++) {
			System.out.print("1 ");
		}
		
		
	}

}
